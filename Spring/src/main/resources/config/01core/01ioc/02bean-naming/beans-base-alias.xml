<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- name a bean alias by 'name' attribute -->
	<bean id="businessDao" name="bd1 bd2 bd3" class="com.study.spring._01core._01ioc._01base_config.BusinessDao"></bean>
	
	<!-- name a bean alias by 'alias' element -->
	<alias name="businessDao" alias="bussDaoA"/>
	<alias name="businessDao" alias="bussDaoB"/>
</beans>