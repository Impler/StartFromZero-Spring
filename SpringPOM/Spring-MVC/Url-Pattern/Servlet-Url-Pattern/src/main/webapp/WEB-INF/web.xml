<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>ExactServlet</servlet-name>
		<display-name>ExactServlet</display-name>
		<servlet-class>com.study.spring.springmvc.urlpattern.servlet.ExactServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ExtensionServlet</servlet-name>
		<display-name>ExtensionServlet</display-name>
		<servlet-class>com.study.spring.springmvc.urlpattern.servlet.ExtensionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DefaultServlet</servlet-name>
		<display-name>DefaultServlet</display-name>
		<servlet-class>com.study.spring.springmvc.urlpattern.servlet.DefaultServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>WildcardServlet</servlet-name>
		<display-name>WildcardServlet</display-name>
		<servlet-class>com.study.spring.springmvc.urlpattern.servlet.WildcardServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ExactServlet</servlet-name>
		<url-pattern>/a/b/c</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ExtensionServlet</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DefaultServlet</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<!-- <servlet-mapping>
		<servlet-name>WildcardServlet</servlet-name>
		<url-pattern>/a/*</url-pattern>
	</servlet-mapping> -->
</web-app>
